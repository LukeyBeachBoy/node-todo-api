{
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": 9
    },
    "env": {
        "node": true,
        "browser": true,
        "es6": true,
        "mocha": true
    },
    "plugins": [
        "mocha"
      ],
    "rules": {
        "linebreak-style":"off",
        "comma-dangle":"off",
        "no-underscore-dangle":"off",
        "func-names":"off",
        "no-console":"off",
        "consistent-return":"off",
        "no-unused-vars": ["error", {"vars": "local", "args": "none"}],
        "mocha/no-exclusive-tests": "error",
        "prefer-destructuring":"off",
        "object-curly-newline":"off"
    }
}